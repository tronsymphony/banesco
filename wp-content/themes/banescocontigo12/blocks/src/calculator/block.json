{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "banescocontigo/calculator",
	"version": "1.0.0",
	"title": "Calculator",
	"category": "widgets",
	"icon": "calculator",
	"supports": {
		"html": false
	},
	"textdomain": "blocks",
	"editorScript": "file:../../../blocks/build/calculator.js",
	"editorStyle": "file:../../../blocks/build/calculator.css",
	"style": "file:../../../blocks/build/style-calculator.css",
	"attributes": {
		"paddings": {
			"type": "object",
			"default": {
				"desktop": {
					"top": "36px",
					"bottom": "35px"
				},
				"mobile": {
					"top": "25px",
					"bottom": "24px"
				}
			}
		},
		"margins": {
			"type": "object",
			"default": {
				"desktop": {
					"top": "0px",
					"bottom": "0px"
				},
				"mobile": {
					"top": "0px",
					"bottom": "0px"
				}
			}
		},
		"gradient": {
			"type": "string",
			"default": "#f7f9fb"
		},
		"title": {
			"type": "string",
			"default": "Tasa de cambio"
		},
		"description": {
			"type": "string",
			"default": "Estas tasas son de referencia. Última actualización"
		},
		"buy": {
			"type": "string",
			"default": "Compra"
		},
		"sell": {
			"type": "string",
			"default": "Venta"
		}
	}
}
