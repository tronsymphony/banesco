{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "banescocontigo/locations",
	"version": "1.0.0",
	"title": "Locations",
	"category": "widgets",
	"icon": "location-alt",
	"supports": {
		"html": false
	},
	"textdomain": "blocks",
	"editorScript": "file:../../../blocks/build/locations.js",
	"editorStyle": "file:../../../blocks/build/locations.css",
	"style": "file:../../../blocks/build/style-locations.css",
	"attributes": {
		"paddings": {
			"type": "object",
			"default": {
				"desktop": {
					"top": "0px",
					"bottom": "0px"
				},
				"mobile": {
					"top": "0px",
					"bottom": "0px"
				}
			}
		},
		"margins": {
			"type": "object",
			"default": {
				"desktop": {
					"top": "0px",
					"bottom": "0px"
				},
				"mobile": {
					"top": "0px",
					"bottom": "0px"
				}
			}
		},
		"gradient": {
			"type": "string",
			"default": "#fff"
		},
		"title": {
			"type": "string",
			"default": "Ubícanos"
		},
		"placeholder": {
			"type": "string",
			"default": "Busca una dirección o localidad "
		}
	}
}
