{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "banescocontigo/other-cards-white",
	"version": "1.0.0",
	"title": "Other Cards White",
	"category": "widgets",
	"icon": "excerpt-view",
	"supports": {
		"html": false
	},
	"textdomain": "blocks",
	"editorScript": "file:../../../blocks/build/other-cards-white.js",
	"editorStyle": "file:../../../blocks/build/other-cards-white.css",
