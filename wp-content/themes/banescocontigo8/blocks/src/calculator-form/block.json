{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "banescocontigo/calculator-form",
	"version": "1.0.0",
	"title": "Calculator form",
	"category": "widgets",
	"icon": "warning",
	"supports": {
		"html": false
	},
	"textdomain": "blocks",
	"editorScript": "file:../../../blocks/build/calculator-form.js",
	"editorStyle": "file:../../../blocks/build/calculator-form.css",
	"style": "file:../../../blocks/build/style-calculator-form.css",
	"attributes": {
		"paddings": {
			"type": "object",
			"default": {
				"desktop": {
					"top": "64px",
					"bottom": "64px"
				},
				"mobile": {
					"top": "50px",
					"bottom": "50px"
				}
			}
		},
		"margins": {
			"type": "object",
			"default": {
				"desktop": {
					"top": "0px",
					"bottom": "0px"
				},
				"mobile": {
					"top": "0px",
					"bottom": "0px"
				}
			}
		},
		"gradient": {
			"type": "string",
			"default": "transparent"
		},
		"title": {
			"type": "string",
			"default": "Calculadora de Puntos"
		}
	}
}
